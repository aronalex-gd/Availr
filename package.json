    "gradient-string": "^3.0.0",
    "hono": "^4.0.0",
    "inquirer": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.0"
